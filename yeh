    getgenv().ED = false;
    function ED()
    spawn(function()
        while wait(1) do
        if not getgenv().ED then break end;
        if game.Players.LocalPlayer.Character then

            if game.Players.LocalPlayer.Charges:FindFirstChild("Experimental Drink").Amount.Value >= 0 then
                if game:GetService("Players").LocalPlayer.Character.Humanoid.Health < 60  then 
                    local args = {
                        [1] = game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Experimental Drink"),
                        [2] = game:GetService("Players").LocalPlayer
                    }
                    game:GetService("ReplicatedStorage").RemoteFunctions.UseConsumable:InvokeServer(unpack(args))
                end
            end
    
                end
            end
        end)
    end

	local ED =  Tab:AddToggle({Name = "Auto Use",Default = false,Callback = function(tool)
		getgenv().ED = tool
		if tool then
			ED();
		end	
	end    
	})

